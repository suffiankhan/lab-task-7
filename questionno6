#include <iostream>
using namespace std;
int getMax(int a[],int n){int m=a[0]; for(int i=1;i<n;i++) if(a[i]>m) m=a[i]; return m;}
void countSortDigit(int a[],int n,int exp){
    int out[20000], count[10]={0};
    for(int i=0;i<n;i++) count[(a[i]/exp)%10]++;
    for(int i=1;i<10;i++) count[i]+=count[i-1];
    for(int i=n-1;i>=0;i--){ int d=(a[i]/exp)%10; out[count[d]-1]=a[i]; count[d]--; }
    for(int i=0;i<n;i++) a[i]=out[i];
}
void radixSort(int a[],int n){
    int m=getMax(a,n);
    for(int exp=1;m/exp>0;exp*=10) countSortDigit(a,n,exp);
}
void mergeArr(int a[],int l,int m,int r){
    int n1=m-l+1, n2=r-m;
    int L[1000], R[1000];
    for(int i=0;i<n1;i++) L[i]=a[l+i];
    for(int j=0;j<n2;j++) R[j]=a[m+1+j];
    int i=0,j=0,k=l;
    while(i<n1 && j<n2) { if(L[i]<=R[j]) a[k++]=L[i++]; else a[k++]=R[j++];}
    while(i<n1) a[k++]=L[i++];
    while(j<n2) a[k++]=R[j++];
}
void mergeSort(int a[],int l,int r){ if(l<r){ int m=(l+r)/2; mergeSort(a,l,m); mergeSort(a,m+1,r); mergeArr(a,l,m,r);} }
int main(){
    int arr1[10], arr2[10];
    for(int i=0;i<10;i++) cin>>arr1[i];
    for(int i=0;i<10;i++) cin>>arr2[i];
    int combined[20];
    for(int i=0;i<10;i++) combined[i]=arr1[i];
    for(int i=0;i<10;i++) combined[10+i]=arr2[i];
    int n=20;
    int rarr[20]; for(int i=0;i<n;i++) rarr[i]=combined[i];
    radixSort(rarr,n);
    cout<<"Radix Sorted combined:\n";
    for(int i=0;i<n;i++) cout<<rarr[i]<<" ";
    cout<<"\n";
    int marr[20]; for(int i=0;i<n;i++) marr[i]=combined[i];
    mergeSort(marr,0,n-1);
    cout<<"Merge Sorted combined:\n";
    for(int i=0;i<n;i++) cout<<marr[i]<<" ";
    cout<<"\n";
    return 0;
}
